{
  "expo": {
    "name": "my-app",
    "slug": "expo-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sarva.expo.expotest"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cc0bd9ba-797f-404e-a1e2-468fd8c15ca2"
      }
    },
    "scheme": "expo-test",
    "owner": "sarva.expo",
    "plugins": [
      ["expo-build-properties", {
        "ios": {
          "useFrameworks": "static"
        }
      }]
    ]
  }
}
